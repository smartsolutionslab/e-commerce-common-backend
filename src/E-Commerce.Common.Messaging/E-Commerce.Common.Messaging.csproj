    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>E_Commerce.Common.Messaging</RootNamespace>
    <Version>1.0.4</Version>
    <Authors>E-Commerce Platform Team</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/your-org/e-commerce-common-backend</RepositoryUrl>
